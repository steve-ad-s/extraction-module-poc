# Environment configuration for extraction module
# Load environment variables from .env file

# Azure SQL Database connections
export AZURE_SQL_SALES_CONN="${AZURE_SQL_SALES_CONN}"
export AZURE_SQL_FINANCE_CONN="${AZURE_SQL_FINANCE_CONN}"
export AZURE_SQL_OPERATIONS_CONN="${AZURE_SQL_OPERATIONS_CONN}"
export AZURE_SQL_INVENTORY_CONN="${AZURE_SQL_INVENTORY_CONN}"
export AZURE_SQL_CUSTOMER_CONN="${AZURE_SQL_CUSTOMER_CONN}"
export AZURE_SQL_SUPPORT_CONN="${AZURE_SQL_SUPPORT_CONN}"
export AZURE_SQL_AUDIT_CONN="${AZURE_SQL_AUDIT_CONN}"
export AZURE_SQL_MARKETING_CONN="${AZURE_SQL_MARKETING_CONN}"
export AZURE_SQL_HR_CONN="${AZURE_SQL_HR_CONN}"
export AZURE_SQL_ANALYTICS_CONN="${AZURE_SQL_ANALYTICS_CONN}"

# API Keys and Authentication
export ZENDESK_API_TOKEN="${ZENDESK_API_TOKEN}"
export ZENDESK_SUBDOMAIN="${ZENDESK_SUBDOMAIN}"
export ZENDESK_EMAIL="${ZENDESK_EMAIL}"

# Destination Configuration
export GCS_BUCKET_URL="${GCS_BUCKET_URL:-gs://your-data-lake/extracts/}"
export GOOGLE_APPLICATION_CREDENTIALS="${GOOGLE_APPLICATION_CREDENTIALS}"

# Default settings
export DEFAULT_FILE_FORMAT="${DEFAULT_FILE_FORMAT:-parquet}"
export DEFAULT_LOAD_TYPE="${DEFAULT_LOAD_TYPE:-incremental}"
export DEFAULT_SCHEMA="${DEFAULT_SCHEMA:-dbo}"
